  <Project rdf:about="https://openmeetings.apache.org/">
    <license rdf:resource="https://www.apache.org/licenses"/>
    <name>Apache OpenMeetings</name>
    <homepage rdf:resource="https://openmeetings.apache.org/"/>
    <asfext:pmc rdf:resource="https://openmeetings.apache.org/" />

    <shortdesc xml:lang="en">
      Video conferencing, instant messaging, white board and collaborative document editing application.
    </shortdesc>

    <description xml:lang="en">
      The Apache OpenMeetings provides video conferencing, instant messaging, white board, 
      collaborative document editing and other groupware tools using API functions 
      of the Red5 Streaming Server for Remoting and Streaming.
    </description>

    <bug-database rdf:resource="https://issues.apache.org/jira/browse/OPENMEETINGS"/>
    <mailing-list rdf:resource="https://openmeetings.apache.org/mailing-lists.html" />
